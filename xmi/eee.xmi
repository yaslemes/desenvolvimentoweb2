<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi:version="2.1" xmlns:xmi="http://www.omg.org/XMI" xmlns:uml="http://www.omg.org/spec/UML/20090901">
  <uml:Model xmi:id="model1" name="DefinirPrecoModel" >
    <packagedElement xmi:type="uml:Package" xmi:id="pkg1" name="UseCasePackage">
      <!-- Ator Autonomo -->
      <packagedElement xmi:type="uml:Actor" xmi:id="actor1" name="Autônomo"/>
      <!-- Ator Sistema -->
      <packagedElement xmi:type="uml:Actor" xmi:id="actor2" name="Sistema"/>

      <!-- Caso de Uso Principal -->
      <packagedElement xmi:type="uml:UseCase" xmi:id="uc1" name="Definir Preço"/>

      <!-- Casos de Uso incluídos -->
      <packagedElement xmi:type="uml:UseCase" xmi:id="uc2" name="Selecionar Serviço"/>
      <packagedElement xmi:type="uml:UseCase" xmi:id="uc3" name="Inserir Valor"/>
      <packagedElement xmi:type="uml:UseCase" xmi:id="uc4" name="Validar Preço"/>
      <packagedElement xmi:type="uml:UseCase" xmi:id="uc5" name="Salvar Preço"/>

      <!-- Relações Include -->
      <packagedElement xmi:type="uml:Include" xmi:id="include1" name="include1" supplier="uc2" client="uc1"/>
      <packagedElement xmi:type="uml:Include" xmi:id="include2" name="include2" supplier="uc3" client="uc1"/>
      <packagedElement xmi:type="uml:Include" xmi:id="include3" name="include3" supplier="uc4" client="uc1"/>
      <packagedElement xmi:type="uml:Include" xmi:id="include4" name="include4" supplier="uc5" client="uc1"/>

      <!-- Associações Autônomo -> Selecionar Serviço e Inserir Valor -->
      <packagedElement xmi:type="uml:Association" xmi:id="assoc1">
        <memberEnd xmi:idref="actor1_end1"/>
        <memberEnd xmi:idref="uc2_end1"/>
      </packagedElement>
      <packagedElement xmi:type="uml:Association" xmi:id="assoc2">
        <memberEnd xmi:idref="actor1_end2"/>
        <memberEnd xmi:idref="uc3_end2"/>
      </packagedElement>

      <!-- Associações Sistema -> Validar Preço e Salvar Preço -->
      <packagedElement xmi:type="uml:Association" xmi:id="assoc3">
        <memberEnd xmi:idref="actor2_end3"/>
        <memberEnd xmi:idref="uc4_end3"/>
      </packagedElement>
      <packagedElement xmi:type="uml:Association" xmi:id="assoc4">
        <memberEnd xmi:idref="actor2_end4"/>
        <memberEnd xmi:idref="uc5_end4"/>
      </packagedElement>

      <!-- Propriedades (memberEnds) para associações -->
      <ownedEnd xmi:id="actor1_end1" type="actor1" aggregation="none" association="assoc1" />
      <ownedEnd xmi:id="uc2_end1" type="uc2" aggregation="none" association="assoc1" />
      <ownedEnd xmi:id="actor1_end2" type="actor1" aggregation="none" association="assoc2" />
      <ownedEnd xmi:id="uc3_end2" type="uc3" aggregation="none" association="assoc2" />
      <ownedEnd xmi:id="actor2_end3" type="actor2" aggregation="none" association="assoc3" />
      <ownedEnd xmi:id="uc4_end3" type="uc4" aggregation="none" association="assoc3" />
      <ownedEnd xmi:id="actor2_end4" type="actor2" aggregation="none" association="assoc4" />
      <ownedEnd xmi:id="uc5_end4" type="uc5" aggregation="none" association="assoc4" />

    </packagedElement>
  </uml:Model>
</XMI>
